source 'https://rubygems.org'

gem 'rake'

gem 'selenium-webdriver'

gem 'cucumber'

gem 'capybara'

gem 'activesupport'

# dependency -> selenium-webdriver 3
# Enables appium support in Capybara (https://github.com/appium/appium_capybara)
gem 'appium_capybara'

# Ruby library for Appium (https://github.com/appium/ruby_lib)
gem 'appium_lib'